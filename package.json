{
  "name": "vslsfs",
  "version": "0.1.0",
  "license": "MIT",
  "description": "FileSystemProvider For VS Code extensions with Live Share",
  "homepage": "https://github.com/tamuratak/vslsfs",
  "main": "./out/src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamuratak/vslsfs.git"
  },
  "dependencies": {
    "vsls": "^1.0.2594"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.21",
    "@types/vscode": "1.42.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "husky": "^3.0.2",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "bin/**/*",
    "out/src/**/*",
    "out/bin/**/*"
  ],
  "keywords": [
    "vsls",
    "Live Share",
    "VS Code"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "clean": "rimraf out/ .eslintcache",
    "lint": "eslint --cache --ext .ts .",
    "lint:fix": "eslint --fix --cache --ext .ts .",
    "test": "mocha --ui tdd out/test/**/*.js",
    "watch": "tsc -p ./ -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --cache --ext .ts ."
    }
  }
}
